{
  "dependencies": {
    "@dealersocket/ds-ui-react": "*",
    "@dealersocket/react-common": "*",
    "@hocs/with-lifecycle": "*",
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "axios": "*",
    "connected-react-router": "*",
    "history": "*",
    "normalizr": "*",
    "react": "*",
    "react-component-queries": "*",
    "react-dnd": "*",
    "react-dnd-html5-backend": "*",
    "react-dom": "*",
    "react-intl": "*",
    "react-redux": "*",
    "react-redux-toastr": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-sizeme": "*",
    "react-splitter-layout": "*",
    "redux": "*",
    "redux-form": "*",
    "redux-saga": "*",
    "reselect": "*",
    "@dealersocket/package-linter": "*",
    "@redux-saga/testing-utils": "*",
    "@storybook/addon-actions": "*",
    "@storybook/addon-backgrounds": "*",
    "@storybook/addon-knobs": "*",
    "@storybook/addon-notes": "*",
    "@storybook/addon-storysource": "*",
    "@storybook/addons": "*",
    "@storybook/react": "*",
    "@svgr/webpack": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "autoprefixer": "*"
  },
  "scripts": {
    "codemod": "yarn codeshift && yarn prettier",
    "codeshift": "cross-env BABEL_ENV=development jscodeshift --extensions js,jsx --parser flow -t node_modules/@dealersocket/ds-ui-react/codemod/v3.0 src --no-babel",
    "dev": "yarn start",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint-package": "ds-package-linter",
    "lint-watch": "chokidar \"src/**/*.js\" -c \"eslint src\" --initial --polling",
    "prettier": "prettier --loglevel warn --write \"{src,src-startup}/**/*.{js,json}\"",
    "prod": "serve -l 3000 -s build",
    "storybook": "start-storybook -p 6006",
    "test-all": "yarn test --watchAll",
    "test-coverage": "yarn test --coverage",
    "test-coverage-watch": "yarn test --coverage --watch",
    "validate": "yarn prettier && yarn lint-package && yarn lint:fix && yarn flow && yarn test --coverage"
  },
  "eslintConfig": {
    "extends": "@dealersocket/ds-react-flow",
    "rules": {
      "@dealersocket/dealersocket/file-names": [
        "error",
        {
          "validFileTypes": ["actions", "constants", "schema"]
        }
      ]
    },
    "ignorePatterns": [".plop", "config/", "scripts/", "postcss.config.js"]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      }
    ],
    "parser": "babel-flow",
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
