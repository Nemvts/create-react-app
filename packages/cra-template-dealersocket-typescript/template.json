      "@material-ui/core": "4.3.3",
      "@material-ui/icons": "4.2.1",
      "@testing-library/react": "^9.3.2",
      "@testing-library/jest-dom": "^4.2.4",
      "@testing-library/user-event": "^7.1.2",
      "@types/node": "^12.0.0",
      "@types/storybook__addon-actions": "3.4.2",
      "@types/storybook__react": "4.0.1",
      "@types/react": "^16.9.0",
      "@types/react-dom": "^16.9.0",
      "@types/jest": "^24.0.0",
      "axios": "0.18.0",
      "connected-react-router": "6.3.1",
      "enzyme": "3.9.0",
      "enzyme-adapter-react-16": "1.9.1",
      "history": "4.7.2",
      "normalizr": "3.3.0",
      "react-component-queries": "2.3.0",
      "react-dnd": "2.6.0",
      "react-dnd-html5-backend": "2.6.0",
      "react-intl": "3.2.0",
      "react-redux": "6.0.1",
      "react-redux-toastr": "7.4.7",
      "react-router": "5.0.1",
      "react-router-dom": "5.0.1",
      "react-sizeme": "2.5.2",
      "react-splitter-layout": "4.0.0",
      "redux": "4.0.1",
      "redux-form": "8.1.0",
      "redux-saga": "1.0.1",
      "reselect": "4.0.0",
      "typescript": "~3.7.2"
    }
  },
  "scripts": {
    "codemod": "yarn codeshift && yarn prettier",
    "codeshift": "cross-env BABEL_ENV=development jscodeshift --extensions js,jsx --parser flow -t node_modules/@dealersocket/ds-ui-react/codemod/v3.0 src --no-babel",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint-package": "ds-package-linter",
    "lint-watch": "chokidar \"src/**/*.js\" -c \"eslint src\" --initial --polling",
    "prettier": "prettier --loglevel warn --write \"{src,src-startup}/**/*.{js,json}\"",
    "prod": "serve -l 3000 -s build",
    "storybook": "start-storybook -p 6006",
    "test-all": "node scripts/test.js --watchAll",
    "test-coverage": "node scripts/test.js --coverage",
    "test-coverage-watch": "node scripts/test.js --coverage --watch",
    "validate": "yarn prettier && yarn lint-package && yarn lint:fix && yarn flow && yarn test --coverage"
  }
}
